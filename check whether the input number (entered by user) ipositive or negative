import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the number: ");
        int num = s.nextInt();

        if(num > 0)
        {
            System.out.println("Postive");
        }
        else
        {
            System.out.println("Negative");
        }
    }
}
